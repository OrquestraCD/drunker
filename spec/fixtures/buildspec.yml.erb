---
version: 0.1
environment_variables:
  plain_text:
    RAILS_ENV: test
    SECRET_KEY_BASE: super secret
phases:
  install:
    commands:
      - bundle install
  build:
    commands:
      - <%= commands.join(" ") %> 1> <%= stdout %> 2> <%= stderr %>; echo $? > <%= exit_status %>
artifacts:
  files:
    - <%= stdout %>
    - <%= stderr %>
    - <%= exit_status %>
